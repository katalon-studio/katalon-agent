{
  "name": "katalon-agent",
  "version": "v2.5.1",
  "description": "",
  "main": "cli.js",
  "scripts": {
    "start": "npm run eslint && node cli.js start-agent",
    "eslint": "eslint -c .eslintrc.yml src --ext .js",
    "eslint:json": "eslint -c .eslintrc.yml src --ext .js -f json -o reports/eslint-report.json",
    "build": "esbuild cli.js --bundle --platform=node --outfile=agent-bundle.js",
    "buildLinux": "npm run build:pkgLinux64 && npm run afterbuild",
    "buildMacos": "npm run build:pkgMacos64 && npm run afterbuild",
    "buildWin": "npm run build:pkgWin64 && npm run build:pkgWin86 && npm run afterbuild",
    "build:serv": "npm run beforebuild && npm run build:pkg && npm run afterbuild",
    "build:pkg": "pkg --targets node14-linux-x64,node14-macos-x64,node14-win-x64,node14-win-x86 --out-path bin cli.js",
    "build:pkgLinux64": "pkg --targets node14-linux-x64 --output bin/cli-linux-x64 cli.js",
    "build:pkgMacos64": "pkg --targets node14-macos-x64 --output bin/cli-macos-x64 cli.js",
    "build:pkgWin64": "pkg --targets node14-win-x64 --output bin/cli-win-x64 cli.js",
    "build:pkgWin86": "pkg --targets node14-win-x86 --output bin/cli-win-x86 cli.js",
    "afterbuild": "npm run copyService && npm run copyStart",
    "copyConfig": "cpy agentconfig_template bin --rename agentconfig",
    "copyService": "cpy nssm.exe service.bat service.sh bin",
    "copyStart": "cpy start.bat start.sh bin",
    "release": "release-it --ci",
    "test:coverage": "jest --passWithNoTests --coverage --forceExit",
    "test:debug": "jest --passWithNoTests --detectOpenHandles --coverage",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "axios-cached-dns-resolve": "^3.3.0",
    "commander": "^7.0.0",
    "compare-versions": "^6.1.0",
    "decompress": "^4.2.1",
    "esbuild": "0.20.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "glob": "^7.2.3",
    "ini": "^4.1.2",
    "ip": "^2.0.1",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "match-url-wildcard": "0.0.5",
    "moment": "^2.30.1",
    "progress": "^2.0.3",
    "properties": "^1.2.1",
    "properties-reader": "^2.3.0",
    "semver": "^7.7.2",
    "simple-git": "^3.24.0",
    "tmp": "^0.2.4",
    "tree-kill": "^1.2.2",
    "triple-beam": "^1.4.1",
    "url-join": "^4.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-transport": "^4.7.0"
  },
  "author": "katalon",
  "license": "ISC",
  "devDependencies": {
    "cpy-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "jest": "^29.7.0",
    "release-it": "^17.2.0"
  }
}
